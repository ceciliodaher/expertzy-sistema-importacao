{
  "name": "expertzy-sistema-importacao",
  "version": "1.0.0",
  "description": "Sistema Brasileiro de Tributação e Precificação de Importação - Expertzy",
  "main": "index.html",
  "scripts": {
    "dev": "node server.js",
    "test": "npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:fase1": "playwright test tests/e2e/fase1-foundation",
    "test:e2e:fase2": "playwright test tests/e2e/fase2-incentivos-entrada", 
    "test:e2e:fase3": "playwright test tests/e2e/fase3-incentivos-saida",
    "test:e2e:fase4": "playwright test tests/e2e/fase4-dashboard",
    "test:e2e:fase5": "playwright test tests/e2e/fase5-regimes",
    "test:e2e:fase6": "playwright test tests/e2e/fase6-integracao",
    "test:xml-real": "playwright test xml-real-processing",
    "test:logs": "playwright test log-validation", 
    "test:init": "playwright test system-initialization",
    "test:performance": "playwright test --project=performance",
    "test:memory": "playwright test tests/e2e/memory",
    "test:coverage": "playwright test --project=coverage",
    "build": "echo 'Building production version...' && npm run test:e2e:fase1",
    "lint": "echo 'Running lint checks...'",
    "serve": "python3 -m http.server 8000",
    "setup": "npm install && npx playwright install"
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "dexie": "^3.2.7",
    "exceljs": "^4.4.0",
    "jspdf": "^2.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "express": "^4.18.2"
  },
  "keywords": [
    "importacao",
    "tributacao",
    "brasil",
    "icms",
    "precificacao",
    "fiscal",
    "incentivos"
  ],
  "author": "Expertzy System",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/ceciliodaher/expertzy-sistema-importacao.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
